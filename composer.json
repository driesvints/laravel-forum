{
    "name": "andreaselia/laravel-forum",
    "description": "A simple bootstrap styled forum package created for ease of use and expansion.",
    "keywords": ["laravel", "forum", "discussion"],
    "license": "MIT",
    "homepage": "https://github.com/AndreasElia/laravel-forum",
    "authors": [
        {
            "name": "Andreas Elia",
            "email": "andreaselia@live.co.uk"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/database": "~5.4.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "AndreasElia\\Forum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AndreasElia\\Tests\\Forum\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "AndreasElia\\Forum\\ForumServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
